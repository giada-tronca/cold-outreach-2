# Server Configuration
PORT=3001
NODE_ENV=development

# Database
DATABASE_URL="postgresql://username:password@localhost:5432/cold_outreach"

# Redis (for job queues)
REDIS_URL="redis://localhost:6379"

# JWT
JWT_SECRET="your-super-secret-jwt-key-here"
JWT_EXPIRES_IN="24h"

# API Keys
OPENROUTER_API_KEY="your-openrouter-api-key"
GOOGLE_GEMINI_API_KEY="your-google-gemini-api-key"
FIRECRAWL_API_KEY="your-firecrawl-api-key"
PROXYCURL_API_KEY="your-proxycurl-api-key"

# Email Configuration (optional for notifications)
SMTP_HOST="smtp.gmail.com"
SMTP_PORT=587
SMTP_USER="your-email@gmail.com"
SMTP_PASS="your-app-password"

# File Upload
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES="text/csv,application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100 