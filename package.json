{
    "name": "cold-outreach-app",
    "version": "1.0.0",
    "description": "AI-powered Cold Outreach Application",
    "scripts": {
        "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
        "dev:backend": "cd cold-outreach-backend && npm run dev",
        "dev:frontend": "cd cold-outreach-frontend && npm run dev",
        "build": "npm run build:backend && npm run build:frontend",
        "build:backend": "cd cold-outreach-backend && npm run build",
        "build:frontend": "cd cold-outreach-frontend && npm run build",
        "install:all": "npm install && cd cold-outreach-backend && npm install && cd ../cold-outreach-frontend && npm install",
        "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
        "start:backend": "cd cold-outreach-backend && npm start",
        "start:frontend": "cd cold-outreach-frontend && npm run preview"
    },
    "devDependencies": {
        "concurrently": "^9.2.0"
    },
    "keywords": [
        "cold-outreach",
        "ai",
        "sales",
        "automation",
        "typescript",
        "react",
        "nodejs"
    ],
    "author": "Cold Outreach Team",
    "license": "MIT"
}
